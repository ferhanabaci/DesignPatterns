@model DesignPattern.Meditor.MediatorPattern.Results.GetProductByIDQueryResult 

@{
    Layout = null;
}

<link href="~/lib\bootstrap\dist\css\bootstrap.css" rel="stylesheet" />
<div class="container">
    <form method="get">
        <label>Ürün ID</label>
        <input type="text" class="form-control" asp-for="ProductID" />
        <br />
        <label>Ürün Adı</label>
        <input type="text" class="form-control" asp-for="ProductName" />
        <br />
        <label>Ürün Stok Sayısı </label>
        <input type="text" class="form-control" asp-for="ProductStock" />
        <br />
        
    </form>
</div>
